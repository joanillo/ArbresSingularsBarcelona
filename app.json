{
  "expo": {
    "name": "ArbresBarcelona",
    "slug": "ArbresBarcelona",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "package": "org.joanillo.ArbresBarcelona",
      "versionCode": 1.0
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
